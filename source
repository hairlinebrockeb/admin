plr.Chatted:connect(function(msg)
	if msg == "/rhalo" then

game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "m1x admin";
			Text = "removing halo";
		})
while wait() do
	        pcall(function()
	            game.Players.LocalPlayer.Character:FindFirstChild('RealHalo').Handle:Destroy()
	        end)
end
end
end)



function bannedppl()
local banned = {'werexzwolf'}


for i,v in pairs(banned) do


if v.Name == plr.Name then

game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "m1x admin";
			Text = "Your Blacklisted!, rejoining..";
		})



local id = game.PlaceId

  game:GetService("TeleportService"):teleport(id, plr)

end
   end
end




bannedppl()


function thanks()


game:GetService("StarterGui"):SetCore("SendNotification", {

Title = "m1x admin";
Text = "You have used m1x's panel. your use is logged in the system. jk";

})


end



function checkpre()

if pre ~= "[" then

local id = game.PlaceId

  game:GetService("TeleportService"):teleport(id, plr)

end
end

checkpre()
